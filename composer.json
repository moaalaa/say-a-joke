{
    "name": "moaalaa/say-a-joke",
    "description": "Say a random funny joke",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Alaa El-Din",
            "email": "moaalaa16@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "MoaAlaa\\SayAJoke\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MoaAlaa\\SayAJoke\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.5",
        "orchestra/testbench": "3.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoaAlaa\\SayAJoke\\JokesServiceProvider"
            ],
            "aliases": {
                "Jokes": "MoaAlaa\\SayAJoke\\Facades\\JokesFacade"
            }
        }
    }
}
